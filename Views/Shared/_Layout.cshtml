                }
                
                // ÿßŸÑÿ≥ŸÖÿßÿ≠ ÿ®ÿßŸÑŸÜÿ≥ÿÆ ŸàÿßŸÑŸÑÿµŸÇ ŸÅŸä ŸÉŸÑ ŸÖŸÉÿßŸÜ
                if (e.ctrlKey && (e.key === 'c' || e.key === 'v' || e.key === 'x' || e.key === 'a')) {
                    // ÿßŸÑÿ≥ŸÖÿßÿ≠ ÿ®ÿßŸÑŸÜÿ≥ÿÆ ŸàÿßŸÑŸÑÿµŸÇ ŸÅŸä ŸÉŸÑ ŸÖŸÉÿßŸÜ
                    return true;
