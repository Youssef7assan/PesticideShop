            // ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑŸÉŸàÿØ ÿßŸÑŸÖÿµÿØÿ±Ÿä
            document.addEventListener('keydown', function(e) {
                if (e.key === 'F12' || (e.ctrlKey && e.shiftKey && e.key === 'I')) {
                    e.preventDefault();
                    showProtectionMessage('ÿ™ŸÖ ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑÿπŸÜÿßÿµÿ±');
                    return false;
                }
            });
            
            // ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑŸÉŸàÿØ ÿßŸÑŸÖÿµÿØÿ±Ÿä
