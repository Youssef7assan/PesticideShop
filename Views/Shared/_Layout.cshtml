            // ŸÖŸÜÿπ ÿßŸÑÿ™ŸÉÿ®Ÿäÿ± ÿ®ÿßŸÑÿ∂ÿ∫ÿ∑ ÿßŸÑŸÖÿ≤ÿØŸàÿ¨
            let lastTouchEnd = 0;
            document.addEventListener('touchend', function(e) {
                const now = (new Date()).getTime();
                if (now - lastTouchEnd <= 300) {
                    e.preventDefault();
                }
                lastTouchEnd = now;
            }, false);

            // ÿ™ŸÖ ÿ•ŸÑÿ∫ÿßÿ° ŸÖÿ±ÿßŸÇÿ®ÿ© ÿ™ÿ∫ŸäŸäÿ± ÿ≠ÿ¨ŸÖ ÿßŸÑŸÜÿßŸÅÿ∞ÿ© ŸÑÿ™ÿ¨ŸÜÿ® ÿ•ÿπÿßÿØÿ© ÿßŸÑÿ™ÿ≠ŸÖŸäŸÑ ÿßŸÑÿ™ŸÑŸÇÿßÿ¶Ÿä
            // setInterval(function() {
            //     if (window.outerHeight - window.innerHeight > 200 || window.outerWidth - window.innerWidth > 200) {
            //         showProtectionMessage('üö´ ÿ™ŸÖ ÿßŸÉÿ™ÿ¥ÿßŸÅ ŸÖÿ≠ÿßŸàŸÑÿ© ŸÅÿ™ÿ≠ ÿ£ÿØŸàÿßÿ™ ÿßŸÑŸÖÿ∑Ÿàÿ±');
            //         // ÿ•ÿπÿßÿØÿ© ÿ™Ÿàÿ¨ŸäŸá ÿ£Ÿà ÿ•ÿ∫ŸÑÿßŸÇ ÿßŸÑŸÜÿßŸÅÿ∞ÿ©
            //         window.location.href = window.location.href;
            //     }
            // }, 1000);

            // ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑÿπŸÜÿßÿµÿ± ÿ®ÿßŸÑŸÖÿßŸàÿ≥ ÿßŸÑÿ£ŸäŸÖŸÜ
            document.addEventListener('mousedown', function(e) {
