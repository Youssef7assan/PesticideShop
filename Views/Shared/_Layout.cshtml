            // ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑÿπŸÜÿßÿµÿ± ÿ®ÿßŸÑÿ∂ÿ∫ÿ∑ ÿßŸÑŸÖÿ≤ÿØŸàÿ¨
            let lastTouchEnd = 0;
            document.addEventListener('touchend', function(e) {
                const now = (new Date()).getTime();
                if (now - lastTouchEnd <= 300) {
                    e.preventDefault();
                    showProtectionMessage('ÿ™ŸÖ ŸÖŸÜÿπ ÿßŸÑŸÅÿ≠ÿµ ÿ®ÿßŸÑÿ∂ÿ∫ÿ∑ ÿßŸÑŸÖÿ≤ÿØŸàÿ¨');
                }
