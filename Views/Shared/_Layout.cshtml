            // ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑÿπŸÜÿßÿµÿ± ÿ®ÿßŸÑŸÖÿßŸàÿ≥
            document.addEventListener('mousedown', function(e) {
                if (e.button === 2) { // Right click
                    e.preventDefault();
                    showProtectionMessage('ÿ™ŸÖ ŸÖŸÜÿπ ÿßŸÑŸÉŸÑŸäŸÉ ÿßŸÑÿ£ŸäŸÖŸÜ');
                    return false;
                }
            });
            
            // ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑÿπŸÜÿßÿµÿ± ÿ®ÿßŸÑŸÑŸÖÿ≥
            document.addEventListener('touchstart', function(e) {
                if (e.touches.length > 1) {
                    e.preventDefault();
                    showProtectionMessage('ÿ™ŸÖ ŸÖŸÜÿπ ÿßŸÑŸÅÿ≠ÿµ ÿ®ÿßŸÑŸÑŸÖÿ≥');
                    return false;
                }
            });
            
            // ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑÿπŸÜÿßÿµÿ± ÿ®ÿßŸÑÿ∂ÿ∫ÿ∑ ÿßŸÑŸÖÿ∑ŸàŸÑ
            let touchTimer;
            document.addEventListener('touchstart', function(e) {
                touchTimer = setTimeout(function() {
                    showProtectionMessage('ÿ™ŸÖ ŸÖŸÜÿπ ÿßŸÑŸÅÿ≠ÿµ ÿ®ÿßŸÑÿ∂ÿ∫ÿ∑ ÿßŸÑŸÖÿ∑ŸàŸÑ');
                }, 1000);
            });
            
            document.addEventListener('touchend', function(e) {
                clearTimeout(touchTimer);
