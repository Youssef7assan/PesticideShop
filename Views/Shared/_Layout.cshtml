                    e.preventDefault();
                    showProtectionMessage('üö´ ÿ™ŸÖ ŸÖŸÜÿπ ÿßŸÑŸÉŸÑŸäŸÉ ÿßŸÑÿ£ŸäŸÖŸÜ');
                    return false;
                }
