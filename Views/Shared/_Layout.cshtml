            // ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑÿπŸÜÿßÿµÿ± ÿ®ÿßŸÑÿ∂ÿ∫ÿ∑ ÿßŸÑŸÖÿ∑ŸàŸÑ ÿπŸÑŸâ ÿßŸÑÿπŸÜÿßÿµÿ±
            document.addEventListener('dragstart', function(e) {
                e.preventDefault();
                showProtectionMessage('ÿ™ŸÖ ŸÖŸÜÿπ ÿßŸÑÿ≥ÿ≠ÿ® ŸàÿßŸÑÿ•ŸÅŸÑÿßÿ™');
                return false;
            });
            
            // ŸÖŸÜÿπ ŸÅÿ≠ÿµ ÿßŸÑÿπŸÜÿßÿµÿ± ÿ®ÿßŸÑÿ∂ÿ∫ÿ∑ ÿßŸÑŸÖÿ∑ŸàŸÑ ÿπŸÑŸâ ÿßŸÑÿπŸÜÿßÿµÿ±
            document.addEventListener('drag', function(e) {
                e.preventDefault();
